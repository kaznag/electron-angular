{
  "name": "electron-angular",
  "version": "0.1.0",
  "main": "main.js",
  "author": "Kaz NAGAMINE",
  "license": "MIT",
  "scripts": {
    "start": "electron ./dist/prod",
    "build": "node ./scripts/package.js && webpack --config ./config/webpack.config.prod.js",
    "clean": "rimraf ./dist/prod",
    "rebuild": "npm run clean && npm run build",
    "start:dev": "electron ./dist/dev",
    "build:dev": "node ./scripts/package.js && webpack --config ./config/webpack.config.dev.js",
    "clean:dev": "rimraf ./dist/dev",
    "rebuild:dev": "npm run clean:dev && npm run build:dev",
    "pack:l": "npm run rebuild && electron-builder --l --config ./config/electron-builder.yml",
    "pack:w": "npm run rebuild && electron-builder --w --config ./config/electron-builder.yml",
    "pretest": "npm run lint",
    "test": "jest --config ./config/jest.config.js",
    "lint": "run-p lint:*",
    "lint:prettier": "prettier --check --config ./config/.prettierrc.yml config/**/*.{js,json,yml} src/**/*",
    "lint:stylelint": "stylelint --config ./config/.stylelintrc.yml src/**/*.{css,sass,scss}",
    "fix": "run-s fix:*",
    "fix:prettier": "prettier --write --config ./config/.prettierrc.yml config/**/*.{js,json,yml} src/**/*",
    "fix:stylelint": "stylelint --config ./config/.stylelintrc.yml --fix src/**/*.{css,sass,scss}",
    "prepare": "husky install"
  },
  "keywords": [
    "electron",
    "angular",
    "webpack",
    "electron-builder",
    "localization",
    "ngx-translate",
    "ngrx",
    "jest"
  ],
  "dependencies": {
    "@angular/common": "^12.1.1",
    "@angular/compiler": "^12.1.1",
    "@angular/core": "^12.1.1",
    "@angular/platform-browser": "^12.1.1",
    "@angular/platform-browser-dynamic": "^12.1.1",
    "@ngrx/effects": "^12.5.1",
    "@ngrx/store": "^12.5.1",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "electron-store": "^8.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.6.3",
    "tslib": "^2.3.0",
    "zone.js": "^0.11.4"
  },
  "devDependencies": {
    "@angular/animations": "^12.1.1",
    "@angular/router": "^12.1.1",
    "@ngrx/store-devtools": "^12.3.0",
    "@testing-library/angular": "^10.9.2",
    "@testing-library/jest-dom": "^5.14.1",
    "angular2-template-loader": "^0.6.2",
    "copy-webpack-plugin": "^9.0.1",
    "css-loader": "^3.6.0",
    "electron": "23.3.13",
    "electron-builder": "^22.11.7",
    "electron-devtools-installer": "^3.2.0",
    "html-loader": "^1.3.2",
    "html-webpack-plugin": "^5.3.2",
    "husky": "^7.0.0",
    "javascript-obfuscator": "^2.16.0",
    "jest": "^26.6.3",
    "jest-preset-angular": "^8.4.0",
    "lint-staged": "^11.0.1",
    "mini-css-extract-plugin": "^2.1.0",
    "npm-run-all2": "^6.1.2",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "sass": "^1.75.0",
    "sass-loader": "^12.1.0",
    "style-loader": "^3.1.0",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-recommended-scss": "^4.3.0",
    "stylelint-order": "^4.1.0",
    "stylelint-scss": "^3.19.0",
    "to-string-loader": "^1.1.6",
    "ts-loader": "^9.2.3",
    "typescript": "^4.3.5",
    "webpack": "^5.44.0",
    "webpack-cli": "^5.1.4",
    "webpack-merge": "^5.8.0",
    "webpack-node-externals": "^3.0.0",
    "webpack-obfuscator": "^3.3.2"
  },
  "volta": {
    "node": "18.20.2",
    "yarn": "1.22.22"
  }
}
