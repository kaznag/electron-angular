{
  "name": "electron-angular",
  "version": "0.1.0",
  "main": "main.js",
  "author": "Kaz NAGAMINE",
  "license": "MIT",
  "scripts": {
    "start": "electron ./dist/prod",
    "build": "node ./scripts/package.js && webpack --config ./config/webpack.config.prod.js",
    "clean": "rimraf ./dist/prod",
    "rebuild": "yarn clean && yarn build",
    "start:dev": "electron ./dist/dev",
    "build:dev": "node ./scripts/package.js && webpack --config ./config/webpack.config.dev.js",
    "clean:dev": "rimraf ./dist/dev",
    "rebuild:dev": "yarn clean:dev && yarn build:dev",
    "pack:l": "yarn rebuild && electron-builder --l --config ./config/electron-builder.yml",
    "pack:w": "yarn rebuild && electron-builder --w --config ./config/electron-builder.yml",
    "format:css": "stylelint --config ./config/.stylelintrc.json --fix ./src/**/*.{css,sass,scss}",
    "test": "jest --config ./config/jest.config.js"
  },
  "keywords": [],
  "dependencies": {
    "@angular/common": "^10.1.6",
    "@angular/compiler": "^10.1.6",
    "@angular/core": "^10.1.6",
    "@angular/platform-browser": "^10.1.6",
    "@angular/platform-browser-dynamic": "^10.1.6",
    "electron-store": "^6.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.6.3",
    "tslib": "^2.0.3",
    "zone.js": "^0.11.2"
  },
  "devDependencies": {
    "@angular/animations": "^10.1.6",
    "@angular/router": "^10.1.6",
    "@testing-library/angular": "^10.3.2",
    "@testing-library/jest-dom": "^5.11.9",
    "angular2-template-loader": "^0.6.2",
    "copy-webpack-plugin": "^6.1.0",
    "css-loader": "^3.6.0",
    "electron": "^10.1.3",
    "electron-builder": "^22.8.0",
    "html-loader": "^1.3.2",
    "html-webpack-plugin": "^4.4.1",
    "husky": "^4.3.0",
    "jest": "^26.6.3",
    "jest-preset-angular": "^8.3.2",
    "lint-staged": "^10.5.3",
    "mini-css-extract-plugin": "^1.0.0",
    "node-sass": "^4.14.1",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "sass-loader": "^10.0.2",
    "style-loader": "^1.2.1",
    "stylelint": "^13.8.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-order": "^4.1.0",
    "stylelint-prettier": "^1.1.2",
    "stylelint-scss": "^3.18.0",
    "stylelint-webpack-plugin": "^2.1.1",
    "to-string-loader": "^1.1.6",
    "ts-loader": "^8.0.3",
    "typescript": "^4.0.2",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-merge": "^5.1.3",
    "webpack-node-externals": "^2.5.2",
    "webpack-obfuscator": "^2.1.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "node ./scripts/package.js && git add ./src/package.json && lint-staged"
    }
  },
  "lint-staged": {
    "./src/**/*.{css,sass,scss}": [
      "stylelint --config ./config/.stylelintrc.json --fix"
    ]
  }
}
