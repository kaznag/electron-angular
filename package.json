{
  "name": "electron-angular",
  "version": "0.1.0",
  "main": "main.js",
  "author": "Kaz NAGAMINE",
  "license": "MIT",
  "scripts": {
    "build": "node ./scripts/package.js && webpack --config ./config/webpack.config.prod.js",
    "start": "electron dist/prod",
    "build:dev": "node ./scripts/package.js && webpack --config ./config/webpack.config.dev.js",
    "start:dev": "electron dist/dev",
    "pack:l": "yarn build && electron-builder --l --config ./config/electron-builder.yml",
    "pack:w": "yarn build && electron-builder --w --config ./config/electron-builder.yml"
  },
  "keywords": [],
  "dependencies": {
    "@angular/common": "7",
    "@angular/compiler": "7",
    "@angular/core": "7",
    "@angular/platform-browser": "7",
    "@angular/platform-browser-dynamic": "7",
    "core-js": "2.6",
    "electron-store": "^5.1.1",
    "rxjs": "6.3",
    "tslib": "^1.11.1",
    "zone.js": "0.8"
  },
  "devDependencies": {
    "angular2-template-loader": "^0.6.2",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.5.1",
    "electron": "8.2.0",
    "electron-builder": "^22.4.1",
    "html-loader": "^1.1.0",
    "html-webpack-plugin": "^4.2.0",
    "husky": "^4.2.5",
    "node-sass": "^4.13.1",
    "raw-loader": "1",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "to-string-loader": "^1.1.6",
    "ts-loader": "^6.2.2",
    "typescript": "^3.8.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-merge": "^4.2.2",
    "webpack-node-externals": "^1.7.2",
    "webpack-obfuscator": "^0.27.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "node ./scripts/package.js && git add ./src/package.json"
    }
  }
}
